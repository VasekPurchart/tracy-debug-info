{
	"name": "nella/tracy-debug-info",
	"description": "Tracy BlueScreen panel for Exception::__debugInfo",
	"keywords": ["tracy", "debuginfo", "exception", "debugger"],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Patrik Votoƒçek",
			"email": "patrik@votocek.cz",
			"homepage": "http://patrik.votocek.cz",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/nella/tracy-debug-info/issues",
		"source": "https://github.com/nella/tracy-debug-info"
	},
	"autoload": {
